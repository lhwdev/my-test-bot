{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "nodenext",
    "allowImportingTsExtensions": true,
    "isolatedModules": true,
    "incremental": true
  },
  "exclude": [
    "**/node_modules"
  ]
}
